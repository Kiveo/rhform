{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": [
		"plugin:jest/recommended",
		"plugin:react/recommended",
		"airbnb"
	],
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": 12,
		"sourceType": "module"
	},
	"plugins": [
		"react",
		"jest"
	],
	"rules": {
		"no-param-reassign": [
			2,
			{
				"props": false
			}
		],
		"react/jsx-filename-extension": [
			1,
			{
				"extensions": [
					".js",
					".jsx"
				]
			}
		]
	}
}